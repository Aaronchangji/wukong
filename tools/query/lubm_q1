PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ub: <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#>
SELECT ?X ?Y ?Z
WHERE
{
TI  <ub#University> ?Y <-
?X  <ub#undergraduateDegreeFrom> ?Y <-
?X <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <ub#GraduateStudent> .
?X  <ub#memberOf> ?Z .
?Z  <ub#subOrganizationOf> ?Y .
?Z <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <ub#Department> .
}


{
?Y <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <ub#University> <-
?X  <ub#undergraduateDegreeFrom> ?Y <-
?X <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <ub#GraduateStudent> .
?X  <ub#memberOf> ?Z .
?Z  <ub#subOrganizationOf> ?Y .
?Z <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <ub#Department> .
}
